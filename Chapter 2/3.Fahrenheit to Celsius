#include <stdio.h>
#include <stdlib.h>
#include <math.h>

#define FREEZING_PT 32.0f
#define SCALE_FACTOR (5.0f / 9.0f)

int main(void) 
// void means main has no arguments
{
	float fahrenheit, celsius;
	
	printf("Enter Fahrenheit temperature: ");
	scanf("%f", &fahrenheit);
	
	celsius = (fahrenheit - FREEZING_PT) * SCALE_FACTOR;
	
	printf("Celsius equivalent: %.1f\n", celsius); // %.1f means one Digit after DOT 10.0<-
	
	return 0;
	
		
}
